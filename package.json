{
  "name": "vite-vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "lint-staged",
    "eslint": "eslint --ext .vue,.js src",
    "eslint:fix": "eslint --fix --ext .vue,.js src",
    "prettier": "prettier src/**/*.vue src/**/*.js",
    "prettier:fix": "prettier src/**/*.vue src/**/*.js --write",
    "stylelint": "stylelint src/**/*.vue src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.vue src/**/*.scss --fix",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "/src/*.{js,css,vue}": [
      "prettier"
    ],
    "*.{vue,css,scss,sass,less}": [
      "stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "^1.38.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "vite": "^2.4.4"
  }
}
